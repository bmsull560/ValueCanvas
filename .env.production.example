# Production Environment Configuration
# Copy this file to .env.production and fill in actual values
# NEVER commit .env.production to version control

# Application
VITE_APP_ENV=production
VITE_APP_URL=https://app.valuecanvas.com
VITE_API_BASE_URL=https://api.valuecanvas.com

# Agent Fabric
VITE_AGENT_API_URL=https://agents.valuecanvas.com/api/agents
VITE_AGENT_API_TIMEOUT=30000
VITE_AGENT_CIRCUIT_BREAKER_ENABLED=true
VITE_AGENT_CIRCUIT_BREAKER_THRESHOLD=5
VITE_AGENT_CIRCUIT_BREAKER_COOLDOWN=60000
VITE_AGENT_LOGGING_ENABLED=true
VITE_AGENT_WEBSOCKET_URL=wss://agents.valuecanvas.com/ws/agents

# Supabase
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-production-service-role-key

# Authentication
JWT_SECRET=your-production-jwt-secret-min-32-chars
VITE_SESSION_TIMEOUT=3600000
VITE_MFA_ENABLED=true

# Security
VITE_HTTPS_ONLY=true
CORS_ALLOWED_ORIGINS=https://app.valuecanvas.com,https://api.valuecanvas.com
RATE_LIMIT_PER_MINUTE=60
CSRF_PROTECTION_ENABLED=true
CSP_ENABLED=true

# HashiCorp Vault
VAULT_ENABLED=true
VAULT_ADDR=https://vault.valuecanvas.com:8200
VAULT_NAMESPACE=valuecanvas
# VAULT_TOKEN should be provided via Kubernetes secrets or IAM

# Monitoring
VITE_SENTRY_ENABLED=true
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=production
VITE_SENTRY_SAMPLE_RATE=0.1

DATADOG_ENABLED=true
DATADOG_API_KEY=your-datadog-api-key

PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090

# Feature Flags
VITE_SDUI_DEBUG=false
VITE_AGENT_FABRIC_ENABLED=true
VITE_WORKFLOW_ENABLED=true
VITE_COMPLIANCE_ENABLED=true
VITE_MULTI_TENANT_ENABLED=true
VITE_USAGE_TRACKING_ENABLED=true
VITE_BILLING_ENABLED=true

# Caching
REDIS_ENABLED=true
REDIS_URL=redis://redis.valuecanvas.internal:6379
CACHE_TTL=3600

# Email
EMAIL_ENABLED=true
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASSWORD=your-sendgrid-api-key
EMAIL_FROM=noreply@valuecanvas.com

# Storage
S3_ENABLED=true
S3_BUCKET_NAME=valuecanvas-production-uploads
S3_REGION=us-east-1
# S3 credentials should be provided via IAM roles

# Development (disabled in production)
VITE_HMR_ENABLED=false
VITE_SOURCE_MAPS=false
VITE_REACT_DEVTOOLS=false
LOG_LEVEL=warn

# Testing (disabled in production)
TEST_MODE=false
VITE_MOCK_AGENTS=false
