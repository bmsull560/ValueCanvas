{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:console": "bash scripts/cleanup-console-logs.sh",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "test:rls": "supabase test db --workdir supabase --tests supabase/tests/database/rls_policies.test.sql supabase/tests/database/multi_tenant_rls.test.sql",
    "test:watch": "vitest",
    "test:perf": "vitest run --config vitest.config.ts --include \"test/performance/**/*.test.ts\"",
    "db:setup": "bash infrastructure/supabase/setup.sh",
    "db:push": "supabase db push",
    "db:link": "supabase link --project-ref ${SUPABASE_PROJECT_ID}",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local > src/types/supabase.ts",
    "db:types:remote": "supabase gen types typescript --linked > src/types/supabase.ts",
    "db:test": "supabase test db",
    "db:validate": "psql $DATABASE_URL -f supabase/tests/database/validate_all_fixes.sql",
    "security:scan": "npm audit --audit-level=high",
    "security:scan:snyk": "snyk test --severity-threshold=high",
    "security:scan:all": "npm run security:scan && npm run security:scan:snyk",
    "storybook:snapshot": "storybook test --watch=false",
    "billing:aggregate": "node -r tsx/cjs src/services/metering/UsageAggregator.ts",
    "billing:submit": "node -r tsx/cjs src/services/metering/UsageSink.ts",
    "billing:setup-stripe": "./infrastructure/billing/stripe-products-setup.sh",
    "billing:test-flow": "./infrastructure/testing/test-billing-flow.sh",
    "backend:dev": "tsx watch src/backend/server.ts",
    "backend:start": "node dist/backend/server.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@playwright/test": "^1.57.0",
    "@sentry/react": "^10.27.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@supabase/supabase-js": "^2.57.4",
    "@types/dompurify": "^3.0.5",
    "cors": "^2.8.5",
    "dompurify": "^3.3.0",
    "express": "^4.18.2",
    "lucide-react": "^0.344.0",
    "lz-string": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.0",
    "redis": "^4.6.11",
    "stripe": "^14.7.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/runner": "^2.1.3",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.35",
    "snyk": "^1.1275.0",
    "storybook": "^8.4.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  }
}
