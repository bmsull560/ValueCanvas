{
  "dashboard": {
    "title": "Agent Performance Dashboard",
    "tags": ["agents", "performance", "observability"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Agent Success Rate",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT time_bucket, agent_type, (high_confidence_count + medium_confidence_count)::float / total_invocations * 100 as success_rate FROM agent_performance_metrics WHERE time_bucket \u003e NOW() - INTERVAL '24 hours' ORDER BY time_bucket",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 2,
        "title": "Agent Response Time (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT time_bucket, agent_type, p50_processing_time_ms as p50, p95_processing_time_ms as p95, p99_processing_time_ms as p99 FROM agent_performance_metrics WHERE time_bucket \u003e NOW() - INTERVAL '24 hours' ORDER BY time_bucket",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        }
      },
      {
        "id": 3,
        "title": "Confidence Score Distribution",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT time_bucket, agent_type, high_confidence_count, medium_confidence_count, low_confidence_count FROM agent_performance_metrics WHERE time_bucket \u003e NOW() - INTERVAL '24 hours' ORDER BY time_bucket",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Hallucination Rate",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT time_bucket, agent_type, hallucination_count::float / total_invocations * 100 as hallucination_rate FROM agent_performance_metrics WHERE time_bucket \u003e NOW() - INTERVAL '24 hours' ORDER BY time_bucket",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 5,
        "title": "Agent Invocations by Type",
        "type": "piechart",
        "gridPos": { "x": 0, "y": 16, "w": 8, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT agent_type, SUM(total_invocations) as total FROM agent_performance_metrics WHERE time_bucket \u003e NOW() - INTERVAL '24 hours' GROUP BY agent_type",
            "format": "table"
          }
        ]
      },
      {
        "id": 6,
        "title": "Average Confidence Score",
        "type": "gauge",
        "gridPos": { "x": 8, "y": 16, "w": 8, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT AVG(avg_confidence_score) as avg_confidence FROM agent_performance_metrics WHERE time_bucket \u003e NOW() - INTERVAL '1 hour'",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.6, "color": "yellow" },
                { "value": 0.8, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Total Invocations (Last Hour)",
        "type": "stat",
        "gridPos": { "x": 16, "y": 16, "w": 8, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT SUM(total_invocations) as total FROM agent_performance_metrics WHERE time_bucket \u003e NOW() - INTERVAL '1 hour'",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ]
  }
}
