# ValueCanvas Staging Secrets Configuration
# Copy this file to secrets.tfvars and fill in your actual values
# DO NOT commit secrets.tfvars to version control

# Database Configuration (if not using Supabase)
db_password = "CHANGE-ME-generate-secure-password"

# Supabase Configuration
# Get these from: https://app.supabase.com/project/_/settings/api
supabase_url         = "https://xxxxx.supabase.co"
supabase_anon_key    = "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.CHANGE-ME"
supabase_service_key = "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.CHANGE-ME"

# LLM Provider API Keys
# Together.ai: https://api.together.xyz/settings/api-keys
together_api_key = "CHANGE-ME-your-together-api-key"

# OpenAI (optional fallback)
# Get from: https://platform.openai.com/api-keys
openai_api_key = "sk-CHANGE-ME-your-openai-api-key"

# JWT Secret for Authentication
# Generate with: openssl rand -base64 32
jwt_secret = "CHANGE-ME-generate-with-openssl-rand"

# AWS Certificate Manager ARN
# Create certificate in ACM for your domain
acm_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/CHANGE-ME"

# DataDog API Key (optional)
# Get from: https://app.datadoghq.com/organization-settings/api-keys
datadog_api_key = ""
